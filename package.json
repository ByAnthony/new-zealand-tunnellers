{
  "name": "newzealandtunnellers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettier": "npx prettier . --write",
    "playwright:ui": "npx playwright test --ui",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint . --fix"
    ]
  },
  "engines": {
    "node": ">=20.16.0 <20.17.0",
    "npm": ">=10"
  },
  "dependencies": {
    "cross-env": "10.1.0",
    "lodash": "4.17.21",
    "mysql2": "3.15.3",
    "next": "16.0.1",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "rc-slider": "11.1.9",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sass": "1.93.3",
    "sharp": "0.34.5",
    "ts-node": "10.9.2"
  },
  "devDependencies": {
    "@eslint/js": "9.39.0",
    "@playwright/test": "1.56.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "20.19.9",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.39.0",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "lint-staged": "15.5.2",
    "typescript": "5.9.3"
  }
}
