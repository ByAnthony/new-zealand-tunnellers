    "next": "^15.3.5",
    "nodemon": "^3.1.5",
    "prettier": "^3.6.2",
    "rc-slider": "^11.1.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^15.2.8",
    "typescript": "^5"
